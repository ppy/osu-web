{
  "name": "ppy/osu-web",
  "description": "osu!web",
  "type": "project",
  "homepage": "https://new.ppy.sh",
  "license": "AGPL-3.0",
  "support": {
    "email": "support@ppy.sh",
    "issues": "https://github.com/ppy/osu-web/issues"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/nanaya/slack-php"
    }
  ],
  "require": {
    "chaseconey/laravel-datadog-helper": "*",
    "doctrine/dbal": "*",
    "egulias/email-validator": "*",
    "elasticsearch/elasticsearch": "~6.0",
    "ezyang/htmlpurifier": "*",
    "graham-campbell/github": "*",
    "guzzlehttp/guzzle": "*",
    "itsgoingd/clockwork": ">=3.0",
    "jenssegers/agent": "^2.6",
    "jonnybarnes/commonmark-linkify": "dev-master",
    "laravel/framework": "5.5.*",
    "laravel/passport": "*",
    "laravel/tinker": "~1.0",
    "laravelcollective/html": "5.5.*",
    "league/commonmark": ">=0.16.0",
    "league/flysystem-aws-s3-v3": "^1.0",
    "league/fractal": "*",
    "league/html-to-markdown": "^4.7",
    "lord/laroute": "*",
    "maknz/slack": "dev-laravel-5.4",
    "mariuzzo/laravel-js-localization": "*",
    "paypal/rest-api-sdk-php": "*",
    "sentry/sentry-laravel": "*",
    "symfony/yaml": "*",
    "webuni/commonmark-table-extension": ">=0.8.0",
    "xsolla/xsolla-sdk-php": "*"
  },
  "require-dev": {
    "beyondcode/laravel-query-detector": "^1.0",
    "filp/whoops": "~2.0",
    "fzaninotto/faker": "~1.4",
    "laravel/dusk": "^2.0",
    "mockery/mockery": "~1.0",
    "phpunit/phpunit": "~6.0",
    "symfony/css-selector": "*",
    "symfony/dom-crawler": "*"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "App\\": "app/"
    },
    "files": [
      "app/helpers.php"
    ]
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ],
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "extra": {
    "laravel": {
      "dont-discover": [
        "laravel/passport"
      ]
    }
  }
}
