{
  "name": "ppy/osu-web",
  "description": "osu!web",
  "type": "project",
  "homepage": "https://new.ppy.sh",
  "license": "AGPL-3.0",
  "support": {
    "email": "support@ppy.sh",
    "issues": "https://github.com/ppy/osu-web/issues"
  },
  "require": {
    "ext-ds": "*",
    "ext-redis": "*",
    "bacon/bacon-qr-code": "^3.0",
    "chaseconey/laravel-datadog-helper": ">=1.2.0",
    "egulias/email-validator": "*",
    "elasticsearch/elasticsearch": "^7.17.x-dev",
    "ezyang/htmlpurifier": "*",
    "firebase/php-jwt": "*",
    "graham-campbell/github": "*",
    "guzzlehttp/guzzle": "*",
    "itsgoingd/clockwork": "^5.1",
    "knuckleswtf/scribe": "^5.1",
    "laravel/framework": "^11.0",
    "laravel/helpers": "*",
    "laravel/octane": "*",
    "laravel/passport": "*",
    "laravel/slack-notification-channel": "*",
    "laravel/tinker": "*",
    "league/commonmark": "^2.0",
    "league/flysystem-aws-s3-v3": "*",
    "league/fractal": "*",
    "league/oauth2-github": "^3.1",
    "league/oauth2-server": "^8.3",
    "maennchen/zipstream-php": "^2.1",
    "mariuzzo/laravel-js-localization": "*",
    "mobiledetect/mobiledetectlib": "^4.8",
    "paypal/paypal-checkout-sdk": "*",
    "reedware/laravel-relation-joins": "^6.0",
    "romanzipp/laravel-turnstile": "^1.3",
    "sentry/sentry-laravel": ">=4.13.0",
    "shopify/shopify-api": "^5.6",
    "spomky-labs/otphp": "^11.3",
    "symfony/yaml": "*",
    "tightenco/ziggy": "^1.8",
    "xsolla/xsolla-sdk-php": "*"
  },
  "require-dev": {
    "beyondcode/laravel-query-detector": "*",
    "dealerdirect/phpcodesniffer-composer-installer": "^1.0",
    "dms/phpunit-arraysubset-asserts": "^0.5.0",
    "laravel/dusk": "*",
    "mockery/mockery": "*",
    "nunomaduro/collision": "^8.1",
    "phpunit/phpunit": ">=10.0",
    "scriptfusion/pip": "^3.5",
    "slevomat/coding-standard": "^8.15",
    "spatie/laravel-ignition": "^2.0",
    "squizlabs/php_codesniffer": "^3.10",
    "symfony/css-selector": "*",
    "symfony/dom-crawler": "*"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "8.4.0"
    },
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "php-http/discovery": false
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  }
}
