{
  "name": "ppy/osu-web",
  "description": "osu!web",
  "type": "project",
  "homepage": "https://new.ppy.sh",
  "license": "AGPL-3.0",
  "support": {
    "email": "support@ppy.sh",
    "issues": "https://github.com/ppy/osu-web/issues"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/nanaya/slack-php"
    },
    {
      "type": "vcs",
      "url": "https://github.com/nanaya/xsolla-sdk-php"
    }
  ],
  "require": {
    "chaseconey/laravel-datadog-helper": ">=1.2.0",
    "doctrine/dbal": "*",
    "egulias/email-validator": "*",
    "elasticsearch/elasticsearch": "^7.5.0",
    "ezyang/htmlpurifier": "*",
    "graham-campbell/github": "*",
    "guzzlehttp/guzzle": "*",
    "itsgoingd/clockwork": ">=3.0",
    "jenssegers/agent": "*",
    "laravel/framework": "6.*",
    "laravel/helpers": "^1.1",
    "laravel/passport": "*",
    "laravel/slack-notification-channel": "^2.0",
    "laravel/tinker": "*",
    "laravelcollective/html": "*",
    "league/commonmark": ">=0.16.0",
    "league/commonmark-ext-autolink": "^1.0",
    "league/commonmark-ext-table": "*",
    "league/flysystem-aws-s3-v3": "*",
    "league/fractal": "*",
    "league/html-to-markdown": "*",
    "maknz/slack": "dev-laravel-any",
    "mariuzzo/laravel-js-localization": "*",
    "mpociot/laravel-apidoc-generator": "*",
    "paypal/rest-api-sdk-php": "*",
    "sentry/sentry-laravel": "*",
    "symfony/yaml": "*",
    "tightenco/ziggy": ">=0.8.1",
    "xsolla/xsolla-sdk-php": ">=4.2.0"
  },
  "require-dev": {
    "beyondcode/laravel-query-detector": "*",
    "filp/whoops": "*",
    "fzaninotto/faker": "*",
    "laravel/dusk": "*",
    "mockery/mockery": "*",
    "phpunit/phpunit": "*",
    "symfony/css-selector": "*",
    "symfony/dom-crawler": "*"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "App\\": "app/"
    },
    "files": [
      "app/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "extra": {
    "laravel": {
      "dont-discover": [
        "laravel/passport"
      ]
    }
  }
}
